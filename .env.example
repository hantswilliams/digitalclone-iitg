# Example .env file for DigitalClone IITG
# Copy this file to .env and fill in your actual credentials

# AWS S3 Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here

# PlayHT API Configuration
PLAYHT_USERID=your_playht_user_id_here
PLAYHT_SECRET=your_playht_api_key_here

# Hugging Face API Token
HUGGING=your_huggingface_api_token_here

# Google OAuth (Optional)
GOOGLE_OAUTH_CLIENT_ID=your_google_client_id_here
GOOGLE_OAUTH_CLIENT_SECRET=your_google_client_secret_here

# Flask Configuration
FLASK_SECRET_KEY=generate_a_secure_random_key_here

# S3 Bucket Name (Change if using a different bucket)
S3_BUCKET_NAME=iitg-mvp
